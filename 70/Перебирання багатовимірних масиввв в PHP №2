С помощью трех вложенных циклов найдите сумму элементов этого массива.

<?php
$arr = [
		[
			[1, 2, 3],
			[6, 7, 8],
			[3, 8, 4],
			[6, 7, 9],
		],
		[
			[9, 1, 2],
			[4, 5, 6],
		],
		[
			[9, 1, 2],
			[4, 5, 6],
			[5, 6, 3],
		],
	];
$sum = 0;

foreach ($arr as $sub) {
  foreach ($sub as $elem) {
    foreach ($elem as $elem1) {
      $sum += $elem1;
    }
  }
}

echo $sum;
